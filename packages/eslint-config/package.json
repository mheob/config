	"repository": {
		"type": "git",
		"url": "https://github.com/mheob/config"
	},
	"license": "MIT",
	"author": "Alexander BÃ¶hm <tools@boehm.work>",
	"exports": {
		".": "./dist/index.js",
		"./base": "./dist/base.js",
		"./next": "./dist/next.js",
		"./react": "./dist/react.js",
		"./package.json": "./package.json"
	},
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"typesVersions": {
		"*": {
			".": [
				"./dist/index.d.ts"
